<div class="desc">
	<div class="name">
		{{this.name}}
	</div>
	{{>item-notes}}
</div>
<div class="usage rollable">{{this.usage}}</div>
<div data-item-id="{{this.parent.id}}" data-attack-id="{{this.id}}" data-type="attack" class="level rollable">{{this.level}}</div>
<div class="parry rollable">{{this.fastResolvedParry}}</div>
<div class="block rollable">{{this.fastResolvedBlock}}</div>
<div data-item-id="{{this.parent.id}}" data-attack-id="{{this.id}}" data-type="damage" class="damage rollable">{{this.fastResolvedDamage}}</div>
<div class="reach">{{this.reach}}</div>
<div class="st">{{this.st}}</div>

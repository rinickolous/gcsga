<div class="attribute-bonus">
	<div class="placeholder"> </div>
	<select name="{{path}}.attribute">
		{{#select this.attribute}}
		{{#each attributes as |v k|}}
		<option value="{{k}}">{{v}}</option>
		{{/each}}
		{{/select}}
	</select>
	<select name="{{path}}.limitation" {{input_lock (eq this.attribute "st" )}}>
		{{#select this.limitation}}
		<option value=""></option>
		{{#each ../config.select.st_limitation as |v k|}}
		<option value="{{k}}">{{localize (concat "gcsga.select.st_limitation" v)}}</option>
		{{/each}}
		{{/select}}
	</select>
</div>

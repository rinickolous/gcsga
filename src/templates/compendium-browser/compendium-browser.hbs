<div class="content-box">
	<nav data-group="primary">
		<a data-tab="landing-page" style="display:none"></a>
		<a data-tab="trait">{{localize "gurps.compendium_browser.trait"}}</a>
		<a data-tab="modifier">{{localize "gurps.compendium_browser.modifier"}}</a>
		<a data-tab="skill">{{localize "gurps.compendium_browser.skill"}}</a>
		<a data-tab="spell">{{localize "gurps.compendium_browser.spell"}}</a>
		<a data-tab="equipment">{{localize "gurps.compendium_browser.equipment"}}</a>
		<a data-tab="eqp_modifier">{{localize "gurps.compendium_browser.eqp_modifier"}}</a>
		<a data-tab="note">{{localize "gurps.compendium_browser.note"}}</a>
	</nav>
	<section class="content">
		<div class="tab" data-group="primary" data-tab="landing-page">
			<div style="text-align center; padding: 2em;">{{localize "gurps.compendium_browser.hint"}}</div>
		</div>

		{{#with trait}}
		<div class="tab browser" data-group="primary" data-tab="trait">
			{{print this}}
			{{> compendium-browser/trait }}
		</div>
		{{/with}}
</div>

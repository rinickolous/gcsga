<div class="desc">
	<div class="name">
		{{this.name}}
	</div>
	{{>item-notes}}
</div>
<div class="usage">{{this.usage}}</div>
<div class="level rollable">{{this.level}}</div>
<div class="accuracy rollable">{{this.accuracy}}</div>
<div class="damage rollable">{{this.damage}}</div>
<div class="range">{{this.range}}</div>
<div class="rate-of-fire">{{this.rate_of_fire}}</div>
<div class="shots">{{this.shots}}</div>
<div class="bulk rollable">{{this.bulk}}</div>
<div class="recoil rollable">{{this.recoil}}</div>
<div class="st">{{this.st}}</div>
